tabby_cat = "\tI'm tabbed in."
persian_cat = "I'm split\non a line."
backslash_cat = "I'm \\ a \\ cat."

# Print using horizontal tab character
fat_cat = """
I'll do a list:
\t* Cat food
\t* Fishies
\t* Catnip\n\t* Grass
"""

# Print using vertical tab character
thin_cat = """
I'll do a list:
\v* Cat food
\v* Fishies
\v* Catnip\n\v* Grass
\b\b* Smoked Salmon
"""

print tabby_cat
print persian_cat
print backslash_cat
print fat_cat
print thin_cat
